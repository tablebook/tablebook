{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "import/extensions": ["error", "ignorePackages"],
        "no-console": 0
    }
}
